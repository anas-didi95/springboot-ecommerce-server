type Pagination {
  totalElements: Int
  totalPages: Int
  pageNumber: Int
  pageSize: Int
}

type ProductType {
  code: String
  description: String
  isDeleted: Boolean
  lastModifiedBy: String
  lastModifiedDate(format: String): String
  version: Int
}

type Query {
  pagination: Pagination
  productTypePage(page: Int = 1, size: Int = 2): [ProductType]
  productTypeList: [ProductType]
}

type Subscription {
  hello: Int
}
