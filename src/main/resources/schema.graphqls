type Pagination {
  totalElements: Int
  totalPages: Int
  pageNumber: Int
  pageSize: Int
}

type ProductType {
  code: String
  description: String
  isDeleted: Boolean
  lastModifiedBy: String
  lastModifiedDate(format: String): String
  version: Int
}

type Query {
  pagination(page: Int! = 1, size: Int! = 10): Pagination
  productTypeList: [ProductType]
}

type Subscription {
  hello: Int
}
